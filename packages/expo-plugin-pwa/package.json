{
  "name": "expo-plugin-pwa",
  "version": "0.0.0",
  "main": "build",
  "preferGlobal": true,
  "keywords": [
    "expo",
    "sharp",
    "optimize",
    "react-native",
    "react-native-web"
  ],
  "description": "Create PWA assets from the terminal",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/expo-plugin-pwa"
  },
  "author": "Expo <support@expo.io>",
  "license": "MIT",
  "bin": {
    "expo-optimize": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build:prod",
    "lint": "eslint .",
    "watch": "yarn run build -w",
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf ./build/"
  },
  "devDependencies": {
    "@expo/babel-preset-cli": "^0.2.5",
    "@expo/config": "^2.5.6",
    "@expo/image-utils": "^0.2.12",
    "@expo/json-file": "^8.2.5",
    "@types/node": "^12.6.8",
    "@types/pngjs": "^3.4.0",
    "@types/svg2png": "^4.1.0",
    "@zeit/ncc": "^0.20.5",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "glob": "7.1.2",
    "pngjs": "^3.4.0",
    "pretty-bytes": "5.2.0",
    "rimraf": "2.6.3",
    "svg2png": "^4.1.1",
    "tempy": "0.3.0",
    "update-check": "1.5.3",
    "validate-npm-package-name": "3.0.0"
  },
  "dependencies": {
    "@pwa/manifest": "^1.1.0"
  }
}
